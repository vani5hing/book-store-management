<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="capybara.bookstoremanagement.ManageBillController">
    <children>
        <TableView fx:id="tableView">
            <columns>
                <TableColumn fx:id="colCustomer" text="Customer" />
                <TableColumn fx:id="colBooks" text="Books" />
                <TableColumn fx:id="colTotalPrice" text="Total Price" />
            </columns>
        </TableView>
        <Button text="Return to Menu" onAction="#handleReturnToMenu" />
    </children>
</VBox>
